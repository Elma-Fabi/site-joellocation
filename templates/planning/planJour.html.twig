						<table id="datatable" class="table table-striped table-bordered" style="width:100%">
							<thead class="text-uppercase">
								<tr>
									<th>solde restant</th>
									<th class="text-nowrap">N° résa</th>
									<th>heure</th>
									<th>dates</th>
									<th>
										arrivée retour</th>
									<th>infos</th>
									<th class="text-nowrap">N° vol</th>
									<th>lieu</th>
									<th>modèle véhicule</th>
									<th>immat</th>
									<th>nom</th>
									<th>commune séjour</th>
									<th>nbr pers</th>
								</tr>
							</thead>


							<tbody></tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
