{% extends 'base2.html.twig' %}

{% block title %}Agence{% endblock %}

{% block content %}
    <h1>Agence</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agence.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ agence.nom }}</td>
            </tr>
            <tr>
                <th>Présentation</th>
                <td>{{ agence.presentation }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ agence.mail }}</td>
            </tr>
            <tr>
                <th>Téléphone</th>
                <td>{{ agence.telephone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('agence_index') }}">retour</a>

    <a href="{{ path('agence_edit', {'id': agence.id}) }}">Modifier</a>

    {{ include('agence/_delete_form.html.twig') }}
