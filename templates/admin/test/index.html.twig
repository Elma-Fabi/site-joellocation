{% extends 'base2.html.twig' %}

{% block title %}Réservation
{% endblock %}


{% block stylesheets %}{% endblock %}
{% block current_page %}

	<h2>
		<i class="fa fa-list"></i>Utilisateurs</h2>

{% endblock %}

{% block content %}

	<div class="x_panel">
		{{ form_start(form) }}
		<div class="row">
			<div class="form-group col-md-4">
				<label for="">Marque :</label>
				{{ form_widget(form.agenceDepart) }}
			</div>
			<div class="form-group col-md-4">
				<label for="">Date mise en service :</label>
				{{ form_widget(form.dateDepart) }}
			</div>
			<div class="form-group col-md-4">
				<label for="">Date mise en location :</label>
				{{ form_widget(form.typeVehicule) }}

			</div>


			<div class="form-group col-md-4">
				<label for="">Type :</label>
				{{ form_widget(form.agenceRetour) }}

			</div>
			<div class="form-group col-md-4">
				<label for="">Détails :</label>
				{{ form_widget(form.dateRetour) }}

			</div>
			<div class="form-group col-md-4">
				<label for="">Carburation :</label>
				{{ form_widget(form.lieuSejour) }}

			</div>

		</div>
		<div class="">
			<button class="btn btn-danger" id="enregistrer">
				<i class="fa fa-book" aria-hidden="true"></i>
				{{ button_label|default('Enregistrer') }}
			</button>
		</div>
		{{ form_end(form) }}

	</div>

</div>{% endblock %}{% block javascripts %}{% endblock  %}
