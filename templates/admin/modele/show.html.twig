{% extends 'base2.html.twig' %}

{% block title %}modele
{% endblock %}

{% block content %}
	<h1>modele</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ modele.id }}</td>
			</tr>
			<tr>
				<th>Libell√©</th>
				<td>{{ modele.libelle }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('modele_index') }}" class="btn btn-primary">
		<i class="fa fa-arrow-left" aria-hidden="true"></i>
		retour</a>
	{% if is_granted("ROLE_ADMIN") %}
		<a href="{{ path('modele_edit', {'id': modele.id}) }}" class="btn btn-success">
			<i class="fa fa-book" aria-hidden="true"></i>
			Modifier</a>
	{% endif %}
	{% if is_granted("ROLE_ADMIN") %}
		{{ include('modele/_delete_form.html.twig') }}
	{% endif %}
{% endblock %}
