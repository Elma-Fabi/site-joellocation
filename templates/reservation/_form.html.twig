{{ form_start(form) }}
<div class="row">
	<div class="form-group col-md-6">
		<label for="">Type :</label>
		{{ form_widget(form.type) }}
	</div>
	<div class="form-group col-md-6">
		<label for="">Client :</label>
		{{ form_widget(form.client) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Date résérvation :</label>
		{{ form_widget(form.date_reservation) }}
	</div>
	<div class="form-group col-md-3">
		<label for="">Date début:</label>
		{{ form_widget(form.date_debut) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Date fin :</label>
		{{ form_widget(form.date_fin) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Mode résérvation :</label>
		{{ form_widget(form.mode_reservation) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Utilisateur :</label>
		{{ form_widget(form.utilisateur) }}

	</div>
	<div class="form-group col-md-3">
		<label for="">Code résérvation :</label>
		{{ form_widget(form.code_reservation) }}
	</div>
	<div class="form-group col-md-3">
		<label for="">Véhicule :</label>
		{{ form_widget(form.vehicule) }}

	</div>
	<div class="form-group col-md-6">
		<label for="">Lieu :</label>
		{{ form_widget(form.lieu) }}

	</div>
	<div class="form-group col-md-6">
		<label for="">Etat résérvation :</label>
		{{ form_widget(form.etat_reservation) }}

	</div>
</div>
<div class="">
	<button class="btn btn-danger">
		<i class="fa fa-book" aria-hidden="true"></i>
		{{ button_label|default('Enregistrer') }}
	</button>
</div>
{{ form_end(form) }}
