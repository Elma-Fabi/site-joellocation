{{ form_start(form) }}
<div class="row">

	<div class="form-group col-md-4">
		<label for="">Date départ :</label>
		{{ form_widget(form.dateDepart) }}
	</div>
	<div class="form-group col-md-4">
		<label for="">Date retour :</label>
		{{ form_widget(form.dateRetour) }}
	</div>
	<div class="form-group col-md-4">
		<label for="">Agence départ :</label>
		{{ form_widget(form.agenceDepart) }}
	</div>
	<div class="form-group col-md-4">
		<label for="">Agence retour :</label>
		{{ form_widget(form.agenceRetour) }}
	</div>
	<div class="form-group col-md-4">
		<label for="">Lieu séjour :</label>
		{{ form_widget(form.lieuSejour) }}
	</div>

	<div class="form-group col-md-4">
		<label for="">Option siège :</label>
		{{ form_widget(form.siege) }}
	</div>
	<div class="form-group col-md-4">
		<label for="">Garantie :</label>
		{{ form_widget(form.garantie) }}
	</div>
	<div class="form-group col-md-4">
		<label for="">Client :</label>
		{{ form_widget(form.client) }}
	</div>
	<div class="form-group col-md-4">
		<label for="">Véhicule :</label>
		{{ form_widget(form.vehicule) }}
	</div>

	<div class="form-group col-md-4">
		<label for="">Conducteur :</label>
		{{ form_widget(form.conducteur) }}
	</div>
</div>
<button class="btn btn-success">
	<i class="fa fa-book" aria-hidden="true"></i>
	{{ button_label|default('Enregistrer') }}</button>
{{ form_end(form) }}
